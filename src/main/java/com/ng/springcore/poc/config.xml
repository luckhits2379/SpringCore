<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<!--this is bean, injecting value using setter injection, using value element -->
	<bean class="com.ng.springcore.poc.StudentDetails" name="studentDetail1">

		<property name="studentPhoneNumber">
			<list>
				<value>1234</value>
				<value>1235</value>
				<value>1236</value>
				<null/>
			</list>

		</property>

		<property name="subject">
			<set>
				<value>Maths</value>
				<value>Hindi</value>
				<value>English</value>
			</set>
		</property>

		<property name="addressMap">
			<map>
				<entry key="home" value="Indore" />
				<entry key="office" value="Bhopal" />
				<entry key="other" value="Mumbai" />
			</map>
		</property>

	</bean>

</beans>